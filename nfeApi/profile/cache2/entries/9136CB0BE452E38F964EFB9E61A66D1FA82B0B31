    <script type="text/javascript">

        $("#pnlAssinar").css("display", "none");
        $("#divErro").css("display", "none");

        setInterval(function () {
            $("#divErro").css("display", "none");
        }, 30000);

        function VincularEventos() {
            $("#ctl00_ContentPlaceHolder1_pnlRealizarManifestacao").on("hide", function () {
                $("#divErro").css("display", "none");
            });
        }

        function assinar() {
            //$("#pnlAssinar").css("display", "block");


            $("#ctl00_ContentPlaceHolder1_pnlErro").css("visibility", "hidden");
            var evento = $("#ctl00_ContentPlaceHolder1_ddlManifestar").val();
            var justificativa = $("#ctl00_ContentPlaceHolder1_iptJustificativa").val().trim();

            var dataAutorizacao = "";

            var cnpj = "";
            if ($("#ctl00_ContentPlaceHolder1_iptCPF").html() == '') {

                cnpj = $("#ctl00_ContentPlaceHolder1_iptCPF").val();
            }
            else {
                cnpj = $("#ctl00_ContentPlaceHolder1_iptCNPJBase").val() + $("#ctl00_ContentPlaceHolder1_iptCNPJ").val();
            }
            var chaveAcesso = "";
            if ($("input[name='ctl00$ContentPlaceHolder1$Escolha']:checked").val() == "rbtSemChave") {
                chaveAcesso = $("#ctl00_ContentPlaceHolder1_hdfChaveSelecionada").val();
                dataAutorizacao = $("#ctl00_ContentPlaceHolder1_hdfDataAutorizacao").val();
            }
            else {
                chaveAcesso = $("#ctl00_ContentPlaceHolder1_iptChaveAcesso").val();
            }

            $.ajax({
                type: "POST",
                url: "manifestacaoDestinatario.aspx/obterHash",
                data: "{'cnpj':'" + cnpj + "', 'chaveAcesso':'" + chaveAcesso + "', 'justificativa':'" + justificativa + "', 'codEvento':'" + evento + "', 'dtAutorizacao':'" + dataAutorizacao + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (jsonResult) {
                    if (jsonResult.d.Status == "alerta") {
                        exibirMensagemErro("alerta", jsonResult.d.Mensagem);
                    }
                    else if (jsonResult.d.Status == "erro") {
                        exibirMensagemErro("erro_sistema", jsonResult.d.Mensagem)
                    }
                    else {
                        $("#pnlAssinar").css("visibility", "visible");
                        $("#pnlAssinar").dialog({ width: 500, dialogClass: 'modalPopup' });
                        $("#divCentral").addClass("modalBackground");

                        $('div#pnlAssinar').on('dialogclose', function (event) {
                            $("#divCentral").removeClass("modalBackground");
                        });

                        var chaveAutenticacao = jsonResult.d.InfoAssinadoc.Hash;
                        var documento = jsonResult.d.InfoAssinadoc.Documento;
                        var encoding = jsonResult.d.InfoAssinadoc.Encoding;
                        var thumbprint = jsonResult.d.InfoAssinadoc.ThumbPrint;
                        var xslt = jsonResult.d.InfoAssinadoc.XSLT;
                        var idTagParaAssinar = jsonResult.d.InfoAssinadoc.IdTagParaAssinar;
                        var tipoCustomizacao = jsonResult.d.InfoAssinadoc.TipoCustomizacaoXML;
                        var timestamp = jsonResult.d.InfoAssinadoc.Timestamp;
                        var sistemaOrigem = jsonResult.d.InfoAssinadoc.SistemaOrigem;

                        $("#ctl00_ContentPlaceHolder1_hdfChaveAutenticacao").val(chaveAutenticacao);
                        $("#ctl00_ContentPlaceHolder1_hdfThumbprint").val(thumbprint);

                        var dadosAssinaturaXML = new assinadoc.DadosAssinaturaXML(documento, encoding, thumbprint, xslt, idTagParaAssinar, '', tipoCustomizacao, timestamp, chaveAutenticacao);
                        assinadoc.Assinar(dadosAssinaturaXML, onSuccess, onCancel, onError, 5, sistemaOrigem);
                    }
                }
            });
        }

        function habilitarManifestacao() {
            var linha = $("#ctl00_ContentPlaceHolder1_hdfLinhaSelecionada").val();
            var evento = $("#ctl00_ContentPlaceHolder1_ddlManifestar").val();

            if (evento == "210210" || evento == "210200") {
                $("#ctl00_ContentPlaceHolder1_iptJustificativa").attr("disabled", true);
                $("#ctl00_ContentPlaceHolder1_iptJustificativa").val('');
            }
            else {
                $("#ctl00_ContentPlaceHolder1_iptJustificativa").attr("disabled", false);
            }

            return;
        }

        function onSuccess(dadosAssinaturaXML) {
            $("#ctl00_ContentPlaceHolder1_hdfChaveAutenticacao").val(dadosAssinaturaXML.chaveAutenticacao);
            $("#ctl00_ContentPlaceHolder1_hdfDocumentoAssinado").val(dadosAssinaturaXML.documento);
            $("#ctl00_ContentPlaceHolder1_hdfStatusAssinatura").val("Assinado");
            $("#aspnetForm").submit();
        }

        function onCancel(msg) {
            $('#pnlAssinar').dialog('close');
        }

        function onError(msg) {
            $('#pnlAssinar').dialog('close');
            exibirMensagemErro("Erro", "Falha no procedimento de assinatura")
        }

        function exibirMensagemErro(tipoErro, msg) {
            $("#divErro div.divMensagem").html("<p>" + msg + "</p>");
            $("#divErro").addClass("painel_" + tipoErro);
            $("#divErro").css("display", "block");
            $('html,body').animate({ scrollTop: 0 }, 'slow');
        }

    </script>


                    </div>
                </div>
            </div>
